@model IEnumerable<MertCanErdemBeltekSinav.Models.Genders>

@{
    ViewData["Title"] = "Index";
}

<h1>Cinsiyetler</h1>

<p>
    <a asp-action="Create">Cinsiyet Ekle</a>
</p>
<table class="table">
    
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.GenderName)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Güncelle</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Detay</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Sil</a>
            </td>
        </tr>
}
    </tbody>
</table>
